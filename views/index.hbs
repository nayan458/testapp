    <style>
         .coro{
            width: 90vw;
            height: 27rem;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .coro_ul{
            width: 80vw;
            /* height: 100%; */
            height: 300px;
            display: flex;
            justify-content: center;
            align-items: center;
            list-style: none;
            overflow-x: scroll;
            
        }
        .coro_ul::-webkit-scrollbar{
            display: none;
        }
        .corosel_li{
            margin: 0px 10px;
            transform: scale(1);
            transition: 1s ease-in-out;
            cursor: pointer;
            animation: scrl 100s infinite;
        }
        .coro_ul:hover .corosel_li{
            animation-play-state: paused;
        }
        .corosel_li:hover{
            transform: scale(1.3);
        }
        @keyframes scrl {
            0%{
                transform: translateX(0);
            }
            50%{
                transform: translateX(calc(75px*18));
            }
            100%{
                transform: translateX(calc(-75px*18));
            }
        }
        .corosel_li img{
            width: 200px;
            height: 200px;
            transform: scale(1);
            transition: 1s ease-in-out;
            border-radius:12px ;
        }
        .corosel_li img:hover{
            transform: scale(1.2);
        }
        #sec_03_coro{
            display: flex;
            justify-content: center;
            align-items: center;
            width: 100%;
            height: 100%;
            position: relative;
        }
    </style>
